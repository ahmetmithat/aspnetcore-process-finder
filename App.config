<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--
      This tool should run in an elevated command prompt
      This tool relies on ProcDump so the full path should be given
    -->
    <add key="ProcDumpFullPath" value="C:\Downloads\SysInternals\ProcDump.exe"/>
    
    <!--
      Use comma seperated valus for process name without extension. E.g.:
      <add key="DotnetCoreProcessNames" value="dotnet.exe, myapp.exe, coreapp.exe"/>
      "dotnet" will be used if this is empty or the key is missing.
    -->
    <add key="DotnetCoreProcessNames" value="dotnet.exe, sample.exe, MyAspNetCoreApp.exe"/>
  
  </appSettings>
</configuration>